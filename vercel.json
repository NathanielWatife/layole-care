{
    "builds": [
        {
            "src": "layole-care/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "100mb", 
                "runtime": "python3.10"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "layole-care/wsgi.py"
        }
    ]
}
